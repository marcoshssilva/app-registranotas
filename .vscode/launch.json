{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run in Android",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "android"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Launch Metro Server",
            "request": "launch",
            "runtimeArgs": [
                "start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
    ]
}